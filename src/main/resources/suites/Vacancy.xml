<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Store" thread-count="1" parallel="false">

    <test name="Vacancy" group-by-instances="true">
        <classes>
            <class name="vacancy.CreateDraftVacancyTest" />
            <class name="vacancy.CreateOnApprovalVacancyTest" />
            <class name="vacancy.CreateAndApproveVacancyTest" />
            <class name="vacancy.ArchiveStatusOfVacancyTest" />
        </classes>
    </test>

    <test name="Responses and Recommendations" group-by-instances="true">
        <classes>
            <class name="vacancy.VacancyResponseTest" />
        </classes>
    </test>

</suite>
